{
  "name": "GiantBombRouter",
  "version": "0.0.1",
  "author": "Giant Bomb Engineering",
  "description": "Handles routing of category pages to custom views",
  "type": "extension",
  "AutoloadNamespaces": {
    "MediaWiki\\Extension\\GiantBombRouter\\": "/"
  },
  "HookHandlers": {
    "RouterHandler": {
      "class": "MediaWiki\\Extension\\GiantBombRouter\\Router",
      "services": ["MainConfig"]
    }
  },
  "Hooks": {
    "BeforePageDisplay": "RouterHandler"
  },
  "manifest_version": 2,
  "requires": {
    "MediaWiki": ">=1.40.0",
    "extensions": {
      "GiantBombRouter": "*"
    }
  }
}
